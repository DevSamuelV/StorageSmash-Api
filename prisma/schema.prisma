// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model MinecraftServer {
  serverId    String  @unique
  serverImage String
  name        String
  port        Int
  ip          String
  memory      Int
  uid         String
  gamemode    String
  maxPlayers  Int
  world       String
  seed        String
  online      Boolean
}

model User {
  uid      String @unique
  username String
  email    String
  photoURL String
}
